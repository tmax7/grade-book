<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grade.book.controllers.ClassOverviewController">
   <children>
      <TableView fx:id="studentTableView" layoutX="100.0" layoutY="112.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="courseNameColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Course " />
          <TableColumn fx:id="coursePeriodColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Period" />
            <TableColumn fx:id="studentIdColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Student ID" />
            <TableColumn fx:id="firstNameColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Last Name" />
            <TableColumn fx:id="percentColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Percent" />
            <TableColumn fx:id="gradeColumn" prefWidth="75.0" style="-fx-alignment: CENTER;" text="Grade" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
